<%- include('partials/header') %>
    <div class="contactBody">
        <h1>Contact</h1>
        <form action="/about" method="post">
            <div class="">
                <label for="txtEmail" class="form-label">Email</label>
                <input class="form-control" type="email" name="txtEmail" id="txtEmail">
            </div>
            <div class="">
                <label for="txtSubject" class="form-label">Subject</label>
                <input class="form-control" type="text" name="txtSubject" id="txtSubject">
            </div>
            <div class="">
                <label for="txtMessage" class="form-label">Message</label>
                <textarea class="form-control" name="txtMessage" id="txtMessage" cols="30" rows="10"></textarea>
            </div>
            <div>
                <button type="submit" class="btn btn-primary">Submit</button>
            </div>
        </form>
    </div>
<%- include('partials/footer') %>